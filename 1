  printf("Enter the password: ");
  scanf("%s",&password);
 
  if(strcmp(username,"admin")==0)
  {
          	if(strcmp(password,"admin")==0)
          	{
                      	printf("\nWelcome, Login successfully!");
          	}
          	else
          	{
                      	printf("\nIncorrect password");
          	}
  }
  else
  {
printf("\nIncorrect User name");
  }
 
